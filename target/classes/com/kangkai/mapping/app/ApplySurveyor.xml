<?xml version="1.0" encoding="UTF-8" ?> 
<!DOCTYPE mapper 
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kangkai.mapper.app.ApplySurveyorMapper">

	

	<insert id="insert" parameterType="com.kangkai.pojo.ApplySurveyor" useGeneratedKeys="true" keyProperty="applySurveyorId">

		INSERT INTO `t_apply_surveyor`(`userId`,`name`,`phone`,`province`,`city`,`gender`,`img`)VALUES
		(#{userId},#{name},#{phone},#{province},#{city},#{gender},#{img});
		
	</insert>
	
	

</mapper>